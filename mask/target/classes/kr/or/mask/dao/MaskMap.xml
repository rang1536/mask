<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mask">

<select id="login" parameterType="Map" resultType="kr.or.mask.domain.User">
	SELECT *
	  FROM MEMBER
	 WHERE ID = #{id}
	   AND PASS = #{pass} 
</select>


</mapper>